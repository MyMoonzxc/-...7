#include<locale.h>
#include<stdio.h>
int main()
{
	setlocale(LC_CTYPE, "RUS");
	printf("123\n");
	printf("1 \n  2 \n   3 \n");
	printf("%d\n %d\n  %d\n   %d\n", 1, 2, 3, 4);
	printf("%10.5f\n", 12.234567);
	printf("Остаток от деления %d на %d равен %d\n", 4, 2, 5 % 2);
	printf("Остаток от деления %d на %d равен %d\n", 7, 5, 7 % 5);
	printf("Произведение %d на %d равно %d\n", 2000, 4, 2000 * 4);
	printf("%g разделить %e равно %f\n", 5, 2000000, 5 / 2000000);
}
